# OpenAI API Key for Embeddings and GPT-4o
OPENAI_API_KEY=sk-proj-...

# Qdrant Configuration (Local or Cloud)
QDRANT_HOST=your-qdrant-cloud-url-or-host
QDRANT_PORT=6333
QDRANT_API_KEY=your-qdrant-api-key

# Redis Configuration (For Memory)
# Option 1: Connection URL (Recommended for Cloud)
REDIS_URL=redis://default:password@host:port
# Option 2: Individual Settings
REDIS_HOST=your-redis-cloud-host
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD=your-redis-password
REDIS_SSL=True

# User Agent for SEC EDGAR (Required)
# Format: "Sample Company Name AdminContact@sample.com"
SEC_USER_AGENT="FinancialResearchAgent contact@example.com"

# Tavily API Key (For Web Search)
TAVILY_API_KEY=tvly-...
